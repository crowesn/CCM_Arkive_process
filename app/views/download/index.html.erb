<% flash.each do |key, msg| %>
  <%= content_tag :div, msg, :class => [key, " message"], :id => "notice_#{key}" %>
<%end%>

<table class='table table-striped'>
  <% Dir["public/tmp/records/*.*"].each do |file| %>
    <tr>
      <td><%= link_to file, file[/\/.*/] %></td>
    </tr>
  <% end %>
</table>
